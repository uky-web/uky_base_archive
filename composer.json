{
  "name": "uky-web/uky_base",
  "description": "A Drupal installation profile for the University of Kentucky",
  "type": "drupal-custom-profile",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors":[
    {
      "name": "Nicole Sand",
      "email": "nicole.sand@uky.edu",
      "role": "Developer"
    },
    {
      "name": "Andy Shooner",
      "email": "andrew.shooner@uky.edu",
      "role": "Developer"
    },
    {
      "name": "Jason Ullstam",
      "email": "jason.ullstam@uky.edu",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://gitlab.com/uky-web/university-web-platform/drupal-8/ukd8-install-profile"
  },
  "repositories": [
  {
    "type": "package",
    "package": {
      "name": "desandro/masonry",
      "version": "4.2.1",
      "type": "drupal-library",
      "dist": {
        "url": "https://github.com/desandro/masonry/archive/v4.2.1.zip",
        "type": "zip"
      }
    }
  },
 {
  "type": "package",
  "package": {
    "name": "desandro/imagesloaded",
    "version": "4.1.4",
    "type": "drupal-library",
    "dist": {
      "url": "https://github.com/desandro/imagesloaded/archive/v4.1.4.zip",
      "type": "zip"
      }
    }
  },
 {
  "type": "package",
  "package": {
    "name": "enyo/dropzone",
    "version": "5.1.1",
    "type": "drupal-library",
    "dist": {
      "url": "https://github.com/enyo/dropzone/archive/v5.1.1.zip",
      "type": "zip"
      }
    }
  },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
  },
      {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
],
  "require-dev": {
    "drupal/drupal-extension": "^3.1",
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "~2.8"
  },
  "require": {
    "drupal/config_installer": "^1.0",
    "oomphinc/composer-installers-extender": "^1.0",
    "drupal/console": "^1.0.2",
    "drupal/core": "^8.8.1",
    "drupal/core-composer-scaffold": "^8.8.1",
    "ahebrank/eck_autolabel": "dev-8.x-1.x",
    "ahebrank/frontback": "dev-master",
    "ahebrank/paragraphs_previewer_popup": "dev-8.x-1.x",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "bower-asset/dropzone": "^5.5",
    "bower-asset/imagesloaded": "^4.1",
    "bower-asset/masonry": "^4.2",
    "drupal/admin_toolbar":"^2.2",
    "drupal/adminimal_admin_toolbar":"^1.11",
    "drupal/externalauth":"^1.3",
    "drupal/auto_entitylabel": "^2.0",
    "drupal/better_formats": "1.x-dev",
    "drupal/block_visibility_groups": "^1.1",
    "drupal/chosen": "^2.5",
    "drupal/devel": "^2.0",
    "drupal/easy_breadcrumb": "^1.6",
    "drupal/eck": "^1.0",
    "drupal/embed": "^1.0",
    "drupal/embederator": "1.x-dev",
    "drupal/entity_browser_enhanced": "^1.0",
    "drupal/entity_embed": "1.x-dev",
    "drupal/entity_reference_widget_helpers": "1.x-dev",
    "drupal/features": "3.*",
    "drupal/field_group": "^3.0",
    "drupal/file_browser": "^1.1",
    "drupal/focal_point": "1.*",
    "drupal/google_tag": "^1.0",
    "drupal/image_style_quality": "^1.3",
    "drupal/inline_entity_form": "^1.0",
    "drupal/jquery_timepicker": "1.x-dev",
    "drupal/ldap":"^3.0@beta",
    "drupal/linkit": "^5.0",
    "drupal/masquerade": "2.*",
    "drupal/metatag": "^1.1",
    "drupal/module_filter":"^3.1",
    "drupal/name": "^1.0@RC",
    "drupal/open_summary": "1.x-dev",
    "drupal/override_node_options": "^2.3",
    "drupal/paragraphs": "^1.0",
    "drupal/pathauto": "1.*",
    "drupal/permissions_filter":"^1.1",
    "drupal/redirect": "^1.0",
    "drupal/redis": "^1.0",
    "drupal/url_embed": "1.*",
    "drupal/video_embed_field": "^2.0",
    "drupal/webform": "^5.0",
    "drupal/xmlsitemap": "^1.0",
    "drush/drush": "^9.0.0",
    "harvesthq/chosen": "1.8.7",
    "drupal/viewfield": "^3.0@alpha",
    "drupal/headline_group": "1.x-dev",
    "drupal/field_group_settings": "^3",
    "drupal/menu_block": "^1.5",
    "drupal/tab_toolbar": "^1.0@beta",
    "drupal/described_link": "1.x-dev",
    "drupal/linkit_telephone": "1.x-dev",
    "drupal/emergency_alerts": "2.x-dev",
    "drupal/fourxx_templates": "1.x-dev",
    "drupal/config_ignore": "^2.1",
    "drupal/search_kint": "^1.0",
    "uky-web/ukd8": "^0.1.6"
  },
  "config":{
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
    "extra": {
        "installer-types": [
        "npm-asset",
        "bower-asset"
       ],
      "patches": {
        "drupal/core": {
          "Node and comment delete action require a plugin type":"https://www.drupal.org/files/issues/2018-09-17/2999737-10_1.patch"
        },
        "drupal/devel": {
        "limit kint levels": "https://gist.githubusercontent.com/ahebrank/b201f88201bf036e65067988b50dc2c3/raw/254f1c68054db34ef0a8108fe7ec7ba3b7fb63da/kint_limit_max_levels.patch"
        },
        "drupal/linkit": {
          "simple matcher patch": "https://www.drupal.org/files/issues/2018-06-20/fix-non-entity-path.interdiff.patch"
        },
        "drupal/auto_entitylabel": {
          "don't use entity provider name": "https://www.drupal.org/files/issues/auto_entitylabel-2923876-7-Broken-when-provider-entityId.patch"
        },
        "drupal/inline_entity_form": {
          "multivalue fix": "https://www.drupal.org/files/issues/2019-12-28/inline_entity_form-2653574-141.patch"
        },
        "drupal/menu_block": {
          "follow active": "https://www.drupal.org/files/issues/2018-05-10/menu_block-follow-2756675-50.patch"
        }
      },
      "composer-exit-on-patch-failure": true,
      "patchLevel": {
        "drupal/core": "-p2"
      }
    }
}
